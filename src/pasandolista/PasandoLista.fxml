         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button fx:id="saveButton" defaultButton="true" layoutX="295.0" layoutY="385.0" maxHeight="25.0" maxWidth="25.0" minHeight="25.0" minWidth="50.0" mnemonicParsing="false" onAction="#save" prefHeight="25.0" prefWidth="25.0" text="SAVE">
